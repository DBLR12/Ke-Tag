{
  "manifest_version": 3,
  "name": "KeTag - New Tab",
  "version": "1.0.0",
  "description": "A minimal, customizable new tab page with portable config.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "favicon"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png"
    }
  }
}
